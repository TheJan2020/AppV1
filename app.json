{
  "expo": {
    "name": "Primewave",
    "slug": "ha-app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/500-black.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-screen-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1b2e"
    },
    "ios": {
      "bundleIdentifier": "ai.primewave.app",
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1b2e"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "ha-app",
    "plugins": [
      "expo-router",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ],
      "expo-secure-store"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "60d12dc4-39fe-4c9b-a456-13111390c3fc"
      }
    },
    "owner": "uzairkhan-12"
  }
}
